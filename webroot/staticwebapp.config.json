{
  "routes": [
    {
      "route": "/Decision.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/test",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/login.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "login.html"
  },
  "responseOverrides": {
    "401": {
      "redirect": "/login.html",
      "statusCode": 302
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline' https://fulltextsearch.norwayeast.azurecontainer.io http://fulltextsearch.norwayeast.azurecontainer.io; object-src 'none'"
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "userDetailsClaim": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/1f39fa86-9689-41da-b81e-d60e10615bf0",
          "clientIdSettingName": "04670b4b-78d4-422b-83cb-553627001cf8",
          "clientSecretSettingName": "__AAD_CLIENT_SECRET__"
        }
      }
    }
  }
}